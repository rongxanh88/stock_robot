- users = get_users

%table.table-striped.table-bordered
  %tr 
    %td User Email Address
    %td Role 
    %td
  - users.each do |user|
    %tr
      %td
        = user.email_address
      %td
        = user.role 
      %td 
        = link_to "Edit", edit_admin_user_path(user), id: "edit_user_#{user.id}"
      %td
        = link_to "Delete", admin_user_path(user), method: "delete", id: "del_user_#{user.id}"
        