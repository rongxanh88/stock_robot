%h2 All Tags
%br
.container-fluid
  .row
    - @tags.each do |tag|
      .tag.col-md-12
        %p
          = link_to tag.title, tag_tickers_path(tag)
        %p 
          = link_to "Edit", edit_tag_path(tag), id: "edit_tag_#{tag.id}"
          = link_to "Delete", tag_path(tag), method: "delete", id: "del_tag_#{tag.id}"